{
  "name": "@jaypie/lambda",
  "version": "1.0.3",
  "author": "Finlayson Studio",
  "main": "dist/jaypie-lambda.cjs.js",
  "module": "dist/jaypie-lambda.esm.js",
  "scripts": {
    "build": "rollup --config",
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "init:deploy": "hygen jaypie workflow-npm",
    "lint": "eslint .",
    "new": "hygen jaypie vite",
    "prepublish": "npm run build",
    "test": "vitest",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.js",
    "test:spec:lambdaHandler": "vitest run ./src/__tests__/lambdaHandler.spec.js"
  },
  "dependencies": {
    "@jaypie/core": "^1.0.20"
  },
  "devDependencies": {
    "@jaypie/testkit": "^1.0.15",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.3.26",
    "hygen": "^6.2.11",
    "jest-extended": "^4.0.2",
    "prettier": "^3.2.5",
    "rollup": "^4.16.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "sort-package-json": "^2.8.0",
    "vitest": "^1.4.0"
  }
}
