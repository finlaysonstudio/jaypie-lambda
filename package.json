  "devDependencies": {
    "@jaypie/testkit": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.3.26",
    "hygen": "^6.2.11",
    "jest-extended": "^4.0.2",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.8.0",
    "vitest": "^1.4.0"
