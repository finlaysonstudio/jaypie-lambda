{
  "name": "@jaypie/lambda",
  "version": "1.0.1",
  "author": "Finlayson Studio",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "babel src --out-dir dist/esm --extensions \".js\" --config-file ./babel.esm.config.json && babel src --out-dir dist/cjs --extensions \".js\" --config-file ./babel.cjs.config.json",
    "format": "npm run format:package && npm run format:lint",
    "format:lint": "eslint --fix .",
    "format:package": "sort-package-json ./package.json",
    "init:deploy": "hygen jaypie workflow-npm",
    "lint": "eslint .",
    "new": "hygen jaypie vite",
    "prepublish": "npm run build",
    "test": "vitest",
    "test:spec:index": "vitest run ./src/__tests__/index.spec.js",
    "test:spec:lambdaHandler": "vitest run ./src/__tests__/lambdaHandler.spec.js"
  },
  "dependencies": {
    "@jaypie/core": "^1.0.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@jaypie/testkit": "^1.0.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.3.26",
    "hygen": "^6.2.11",
    "jest-extended": "^4.0.2",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.8.0",
    "vitest": "^1.4.0"
  }
}
